{
  "name": "telemedicine-chatbot-vite",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "tsx server/index.ts",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "health:check": "curl http://localhost:3001/health/ai",
    "test:ai": "node -e \"import('./server/lib/ai-service-manager.js').then(m => m.aiServiceManager.healthCheck().then(console.log))\"",
    "whatsapp:init": "node -e \"import('./server/lib/whatsapp-service.js').then(m => m.whatsappService.initialize())\"",
    "stats:demo": "node -e \"import('./server/lib/stats-service.js').then(m => m.statsService.getDashboardStats().then(console.log))\"",
    "reset:deployment": "./scripts/reset-deployment.sh",
    "reset:pm2": "./scripts/reset-pm2.sh",
    "backup:database": "./scripts/backup-database.sh",
    "reset:docker": "./scripts/docker-reset.sh",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop telemedicine-chatbot",
    "pm2:restart": "pm2 restart telemedicine-chatbot",
    "pm2:logs": "pm2 logs telemedicine-chatbot",
    "pm2:status": "pm2 list",
    "pm2:monit": "pm2 monit"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.263.1",
    "puppeteer": "^21.5.0",
    "qrcode": "^1.5.3",
    "qrcode-terminal": "^0.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-router-dom": "^6.8.0",
    "tailwind-merge": "^2.0.0",
    "whatsapp-web.js": "^1.23.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.6.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
