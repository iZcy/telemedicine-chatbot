{
  "name": "telemedicine-chatbot-vite",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "tsx server/index.ts",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "health:check": "curl http://localhost:3001/health/ai",
    "test:ai": "node -e \"import('./server/lib/ai-service-manager.js').then(m => m.aiServiceManager.healthCheck().then(console.log))\""
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
